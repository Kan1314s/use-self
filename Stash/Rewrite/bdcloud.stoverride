name: 百度云盘
desc: 解锁倍速@Nobyda

http:
  script:
    - match: https:\/\/pan\.baidu\.com\/rest\/2\.0\/membership\/user
      name: bdcloud.js
      type: response
      require-body: true
      timeout: 10
  mitm:
    - 'pan.baidu.com'

script-providers:
  bdcloud.js:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/BaiduCloud.js
    interval: 86400
